{
  "name": "parse-import",
  "version": "0.0.7",
  "description": "A tool to help parse imports and all the dependencies associated with a file.",
  "main": "parse-import",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "parse-import": "./node_modules/.bin/parse-import"
  },
  "scripts": {
    "postinstall": "./scripts/go-npm install",
    "preuninstall": "./scripts/go-npm uninstall",
    "test": "go build -o parse-import && ./parse-import -h"
  },
  "goBinary": {
    "name": "parse-import",
    "path": "./node_modules/.bin",
    "url": "https://github.com/SafalPandey/parse-import/releases/download/{{version}}/parse-import_{{version}}_{{platform}}_{{arch}}.tar.gz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SafalPandey/parse-import.git"
  },
  "author": "Safal Raj Pandey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SafalPandey/parse-import/issues"
  },
  "homepage": "https://github.com/SafalPandey/parse-import#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "request": "^2.81.0",
    "tar": "^2.2.1"
  }
}
